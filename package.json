    "eslint-plugin-tsdoc": "^0.2.10",
    "eslint-plugin-unicorn": "^26.0.1",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "node-loader": "^1.0.2",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.14",
