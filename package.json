{
  "name": "hundredpoints",
  "displayName": "Hundredpoints vscode",
  "description": "",
  "version": "0.0.1",
  "publisher": "HundredPoints",
  "engines": {
    "vscode": "^1.45.1"
  },
  "main": "./dist/extension.js",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "catCoding.start",
        "title": "Start cat coding session",
        "category": "Cat Coding"
      },
      {
        "command": "catCoding.doRefactor",
        "title": "Do some refactoring",
        "category": "Cat Coding"
      }
    ]
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "test-package": "npm run vscode:prepublish && vsce package --no-yarn"
  },
  "devDependencies": {
    "@hundredpoints/eslint-plugin-hundredpoints": "0.0.6",
    "@types/node": "^13.13.11",
    "@types/node-fetch": "^2.5.7",
    "@types/path-is-inside": "^1.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/vscode": "^1.45.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-tsdoc": "^0.2.5",
    "eslint-plugin-unicorn": "^20.1.0",
    "glob": "^7.1.6",
    "node-loader": "^0.6.0",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "vsce": "^1.81.0",
    "vscode-test": "^1.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "keytar": "^6.0.1",
    "node-fetch": "^2.6.0",
    "openid-client": "^3.15.2",
    "path-is-inside": "^1.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
