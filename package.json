{
  "name": "hundredpoints",
  "displayName": "Hundredpoints",
  "description": "",
  "version": "0.0.1",
  "publisher": "HundredPoints",
  "engines": {
    "vscode": "^1.45.1"
  },
  "main": "./dist/extension.js",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "hundredpoints.login",
        "title": "Login",
        "category": "HundredPoints"
      },
      {
        "command": "hundredpoints.clearCredentials",
        "title": "Clear credentials",
        "category": "HundredPoints"
      }
    ]
  },
  "scripts": {
